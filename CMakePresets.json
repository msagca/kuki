{
  "version": 3,
  "configurePresets": [
    {
      "name": "Debug",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "Release",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_C_FLAGS": "-O3 -march=native",
        "CMAKE_CXX_FLAGS": "-O3 -march=native"
      }
    }
  ]
}
